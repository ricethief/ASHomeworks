<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.edu.tafesa.itstudies.groupsmsversion1">




    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GroupSMS">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditMessage"
            android:label="@string/edit_message"></activity>

        <activity
            android:name=".EditSentTo"
            android:label="@string/edit_sendTo"></activity>
    </application>
    <uses-permission android:name="android.permission.SEND_SMS"/>
</manifest>